<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.viewbindingdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jetpack">
        <activity
            android:name=".LaunchActivity"
            android:label="ViewBinding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="Activity中使用ViewBinding" />
        <activity
            android:name=".FrgActivity"
            android:label="Fragment中使用ViewBinding" />
        <activity
            android:name=".RvActivity"
            android:label="RecyclerView中使用ViewBinding" />
        <activity
            android:name=".IncludeActivity"
            android:label="include中使用ViewBinding" />
    </application>

</manifest>