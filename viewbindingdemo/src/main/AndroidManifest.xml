<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.viewbindingdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Jetpack">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="ViewBinding">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".simple.SimpleActivity" />
        <activity android:name=".simple.SimpleFragmentActivity" />
        <activity android:name=".rv.RvActivity" />
        <activity android:name=".include.VBIncludeActivity" />
        <activity android:name="com.example.viewbindingdemo.encapsulation.base.OneActivity" />
        <activity android:name="com.example.viewbindingdemo.encapsulation.reflect.TwoActivity" />
        <activity android:name="com.example.viewbindingdemo.encapsulation.delegate.ThreeActivity" />
    </application>

</manifest>